<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:id="@+id/main_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/appBG"
    tools:context="com.fraserbrooks.progresstracker.MainActivity">




    <com.fraserbrooks.progresstracker.TouchInterceptor
        android:id="@+id/tracker_list"
        tools:listitem="@layout/tracker_item_small"
        tools:listheader="@layout/bar_graph_item"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="9"
        android:layout_marginTop="8dp"
        android:divider="@null"
        android:dividerHeight="0dp">

    </com.fraserbrooks.progresstracker.TouchInterceptor>








</LinearLayout>
